[]  lm:mapping [ lm:name "http://www.ibspan.waw.pl/tss/Money" ; 	lm:altName "file:ontology/money.owl" ] .
[]  lm:mapping [ lm:name "http://www.ibspan.waw.pl/tss/Location" ;	lm:altName "file:ontology/location.owl" ] .
[]  lm:mapping [ lm:name "http://www.ibspan.waw.pl/tss/DataModel" ;	lm:altName "file:ontology/data-model.owl" ] .

